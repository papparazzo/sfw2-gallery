<h1 class="pb-5">{{title}}</h1>
<p>
{{description}}
</p>
<div class="row row-cols-1 row-cols-md-2 g-4">
    {{#each entries}}
    <div class="col">
        <div class="card" style="width: 27rem;">
            <div class="ratio ratio-16x9">
                <iframe src="{{url}}" title="video" allowfullscreen></iframe>
            </div>
            <div class="card-body">
                <p class="card-text">{{text}}</p>
            </div>
        </div>
    </div>
    {{/each}}
</div>




